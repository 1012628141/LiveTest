<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.readyidu.mapper.PhoneDeviceMapper">


    <select id="listByDeviceId" parameterType="java.lang.String" resultType="com.readyidu.model.PhoneDevice">
        SELECT
         id,phone_name AS phoneName,
         phone_id as phoneId,
         device_id as deviceId,
         user_id as userId
         FROM t_phone_device
         WHERE device_id = #{deviceId}
    </select>


</mapper>